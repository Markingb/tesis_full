<!DOCTYPE html>
<html>

    <%- include('layout/header'); %>

<body>

    <%- include('layout/navbar'); %>
    <div class="container">
        <div class="columns">
            <%- include('layout/sidebar'); %>
            <div class="column is-9">
                <%- include('layout/user'); %>    
                
                <% if (data.careers) { %>
                    <div class="field">
                        <div class="control has-icons-left">
                          <div class="select">
                            <select id="careers">
                              <option disabled selected>Carrera</option>
                              <% data.careers.forEach(function(career){ %> 
                                <option><%= career %> </option>
                              <% }); %>
                            </select>
                          </div>

                        </div>
                      </div>
                   
                <% } else {%>
                    No career yet available
                <% } %>
                <div class="is-full">
                    <canvas id="myChart" width="400" height="300"></canvas>
                </div> 
                                   
            </div>
            
        </div>
        
        </div>
    </div>
    <%- include('layout/scripts'); %>
    <script type="text/javascript" src="/subjects.js"> </script>

</body>

</html>